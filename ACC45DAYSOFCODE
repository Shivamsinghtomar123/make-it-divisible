#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin >> t;
	while(t--){
	    int n, i;
	    cin >> n;
	    if(n == 1){
	        cout << 3 << endl;
	        continue;
	    }
	    cout << 1;
	    for(i=1; i<=n-2; i++){
	        cout << 0;
	    }
	    cout << 5 << endl;
	    
	}
	return 0;
}
